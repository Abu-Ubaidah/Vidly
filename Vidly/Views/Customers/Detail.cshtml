@model Vidly.Models.Customer
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@Model.Name</h2>

<ul>

    <li><h2>MembershipType: @Model.MembershipType.Name</h2></li>



    @if (Model.BirthDate != null)
